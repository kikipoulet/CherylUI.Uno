<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="QuicksandRegular">ms-appx:///Cheryl.Uno/Assets/Quicksand-Regular.ttf#Quicksand Regular</FontFamily>
    <FontFamily x:Key="QuicksandBold">ms-appx:///Cheryl.Uno/Assets/Quicksand-SemiBold.ttf#Quicksand SemiBold</FontFamily>

    <Style TargetType="TextBlock">
        <Setter Property="FontFamily" Value="{StaticResource QuicksandRegular}" />
    </Style>
</ResourceDictionary>

